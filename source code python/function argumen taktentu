def total(*bilangan,**keywords): #bintang (*) --> jumlah argumen tak tentu #
    hitung=0
    for bil in bilangan:
        hitung+=bil #hitung=hitung+bil#
    for key in keywords:
        hitung+=keywords[key]

    print hitung

total(1,2,3,4,5)
print''
total(daging=2,sayur=10,buah=3)
print ''
total (7,8,5,daging=2,sayur=10,buah=3)
